extends base

block content 
    section.movies-section
        .row#main
            each movie in searchMovie 
                .col.span-1-of-5.card#imgg
                    img(src=`/resources/img/movieimg/${movie.movieImage}` alt=`${movie.movieName}`)
                    a(href=`/movie/${movie.slug}` )
                        h3= movie.movieName
            
